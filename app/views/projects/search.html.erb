
<div class= "span6 center">
  <br>
    <%= form_tag projects_path, :method => 'get' do %>
      <p> <%= text_field_tag :search, params[:search] %>
        <%= submit_tag "Search", :name => nil %>
      </p>
    <% end %>
  
  <br>


  <% for project in @projects %>
    <h3><%= link_to project.title, project_path(project) %> <h3>
  <% end %>

</div>


